start = content_rating

content_rating = element content_rating {
  attribute type { "oars-1.1" },
  content_attribute*
}
content_attribute = element content_attribute {
  attribute id { ids },
  values
}

values = "unknown" | "none" | "mild" | "moderate" | "intense"
ids = "drugs-alcohol" |
